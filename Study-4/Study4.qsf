t}\n\t}\n\n\t\/\/ Initially disable the NextButton\n\tcheckSelections();\n\t\n\n\t\n\t    for (var i = 0; i < rows; i++) {\n        var row = document.createElement('tr');\n        \tfor (var j = 0; j < cols; j++) {\n            var index = i * cols + j;\n\n\t\t\tvar biography = biographies[index];\n\n            var cell = document.createElement('td');\n\t\t\tif (biography && previousChoices.includes(biography.name)) {\n                cell.classList.add('previous-choice');\n                cell.style.backgroundColor = 'gray';\n            }\n\n            if (index < biographies.length) {\n                var biography = biographies[index];\n\n                \/\/ Create img element and add the link\n                var img = document.createElement('img');\n                img.src = biography.link;\n                img.style.width = '100px';\n\t\t\t\timg.style.display = 'block'; \/\/ To ensure it takes up the full width when centered\n            \timg.style.margin = '0 auto'; \/\/ To center the image\n\n\t\t\t\/\/ Add img and other details to cell\n\t\t\tcell.innerHTML = img.outerHTML + \"<br>\" + \n\t\t\t\tbiography.about + \"<br>\" + \n\t\t\t\t\"<b>\" + biography.name + \"<\/b><br>\" + \n\t\t\t\tbiography.pages + \"<br>\" + \n\t\t\t\tbiography.published + \"<br>\" + \n\t\t\t\t\"<u>\" + biography.who + \"<\/u><br>\" +\n\t\t\t\tbiography.description + \"\";\n\n                \/\/ Add click event to select the biography from the table\n                cell.addEventListener('click', function(biographyName) {\n                    return function() {\n                        handleBiographyClick(this, biographyName);\n                    };\n                }(biography.name)); \/\/ Pass the current biography name into the event function\n            }\n\n            \/\/ Style cell\n            cell.style.border = '1px solid black';\n            cell.style.padding = '10px';\n            cell.style.cursor = 'pointer';\n\t\t\tcell.style.textAlign = 'center';     \/\/ 