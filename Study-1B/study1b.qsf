{\n            var index = i * cols + j;\n\n\t\t\tvar movie = movies[index];\n\n            var cell = document.createElement('td');\n\t\t\tif (movie && previousChoices.includes(movie.title)) {\n                cell.classList.add('previous-choice');\n                cell.style.backgroundColor = 'gray';\n            }\n\n            if (index < movies.length) {\n                var movie = movies[index];\n\n                \/\/ Create img element and add the link\n                var img = document.createElement('img');\n                img.src = movie.link;\n                img.style.width = '100px';\n\t\t\t\timg.style.display = 'block'; \/\/ To ensure it takes up the full width when centered\n            \timg.style.margin = '0 auto'; \/\/ To center the image\n\n                \/\/ Add img and other details to cell\n\t\t\tcell.innerHTML = img.outerHTML + \"<br>\" + \n\t\t\t\tmovie.about + \"<br>\" + \n\t\t\t\t\"<b>\" + movie.name + \"<\/b><br>\" + \n\t\t\t\t\"<u>\" + movie.who + \"<\/u><br>\" + \n\t\t\t\tmovie.description + \"<br>\" +  \n\t\t\t\t\"<u>Year:<\/u> \" + movie.year + \"<br>\" + \n\t\t\t\t\"<u>Budget:<\/u> \" + movie.budget + \"<br>\"  + \"<u>Length:<\/u> \" + movie.length + \"\";\n\n                \/\/ Add click event to select the movie from the table\n                cell.addEventListener('click', function(movieTitle) {\n                    return function() {\n                        handleMovieClick(this, movieTitle);\n                    };\n                }(movie.title)); \/\/ Pass the current movie title into the event function\n            }\n\n            \/\/ Style cell\n            cell.style.border = '1px solid black';\n            cell.style.padding = '10px';\n            cell.style.cursor = 'pointer';\n\t\t\tcell.style.textAlign = 'center';     \/\/ Center align content horizontally\n        \tcell.style.verticalAlign = 'top';    \/\/ Top align content vertically\n            row.appendChild(cell);\n        }\n        table.appendChild(