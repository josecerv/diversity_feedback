 in terms of ';\n            if (metConditions.length === 1) {\n                text += metConditions[0] + '.';\n            } else {\n                var lastCondition = metConditions.pop();\n                text += metConditions.join(\", \") + ', and ' + lastCondition + '.';\n            }\n        }\n    }\n\n    \/\/ Set text based on the group value\n    switch (group) {\n        case 'encourage':\n            \/\/ Retrieve the feedback items\n            var feedbackItem1 = Qualtrics.SurveyEngine.getEmbeddedData('feedbackItem1');\n            var feedbackItem2 = Qualtrics.SurveyEngine.getEmbeddedData('feedbackItem2');\n            var feedbackItem3 = Qualtrics.SurveyEngine.getEmbeddedData('feedbackItem3');\n            var feedbackItems = [feedbackItem1, feedbackItem2, feedbackItem3];\n            setTextBasedOnFeedback(feedbackItems);\n            break;\n        case 'base':\n        default:\n            text = ''; \/\/ Leave text empty\n            break;\n    }\n\n    \/\/ Save the text as embedded data\n    Qualtrics.SurveyEngine.setEmbeddedData('text', text);\n    console.log(text); \n});\n\nQualtrics.SurveyEngine.addOnReady(function()\n{\n\t\/*Place your JavaScript here to run when the page is fully displayed*\/\n\n});\n\nQualtrics.SurveyEngine.addOnUnload(function()\n{\n\t\/*Place your JavaScript here to run when the page is unloaded*\/\n\n});"}}]}