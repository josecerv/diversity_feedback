eedbackText += \"<li>\" + feedbackItems[i] + \"<\/li>\";\n    }\n    feedbackText += \"<\/ul>\";\n\n    \/\/ Add the feedback to the question\n    this.getQuestionContainer().innerHTML += feedbackText;\n    \n    for (let i = 0; i < feedbackItems.length; i++) {\n        Qualtrics.SurveyEngine.setEmbeddedData('feedbackItem' + (i+1), feedbackItems[i]);\n    }\n});\n\nQualtrics.SurveyEngine.addOnReady(function() {\n    \/* Additional logic for when the page is fully displayed *\/\n});\n\nQualtrics.SurveyEngine.addOnUnload(function() {\n    \/* Additional logic for when the page is unloaded *\/\n});","DataVisibility":{"Private":false,"Hidden":false}}},{"SurveyID":"SV_2tycLNwc08A12cK","Element":"SQ","PrimaryAttribute":"QID10","SecondaryAttribute":"Thank you for your selections. We just have a few more questions.","TertiaryAttribute":null,"Payload":{"ChoiceOrder":[],"Configuration":{"QuestionDescriptionOption":"UseText"},"DataExportTag":"Q23","DataVisibility":{"Hidden":false,"Private":false},"DefaultChoices":false,"GradingData":[],"Language":[],"NextAnswerId":1,"NextChoiceId":4,"QuestionDescription":"Thank you for your selections. We just have a few more questions.","QuestionID":"QID10","QuestionText":"Thank you for your selections. We just have a few more questions.<br>","QuestionText_Unsafe":"Thank you. In the following section please answer the question to the best of your ability.<br>","QuestionType":"DB","Selector":"TB","Validation":{"Settings":{"Type":"None"}}}},{"SurveyID":"SV_2tycLNwc08A12cK","Element":"SQ","PrimaryAttribute":"QID12","SecondaryAttribute":"Thank you. We have a few more questions for you about your panel decision.","TertiaryAttribute":null,"Payload":{"ChoiceOrder":[],"Configuration":{"QuestionDescriptionOption":"UseText"},"DataExportTag":"Q32","DataVisibility":{"Hidden":false,"Private":false},"DefaultChoices":false,"GradingData":[],"Language":[],"NextAnswerId":1,"NextChoiceId":4,"QuestionDescription":"Thank you. We have a few more questions for you 