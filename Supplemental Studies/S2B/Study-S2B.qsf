ra.style.fontWeight = 'bold';\n    instructionPara.style.fontSize = '18px';\n    this.getQuestionContainer().appendChild(instructionPara);\n\n    \/\/ Table creation\n    var table = document.createElement('table');\n    table.style.borderCollapse = 'collapse';\n    table.style.width = '100%';\n    table.style.color = 'black';\n\n    \/\/ Calculate rows and columns\n    var rows = 5;\n    var cols = 5;\n    var selectedPodcasts = [];\n\n\t\/\/ Function to handle click event on podcast cell\n\tfunction handlePodcastClick(cell, podcastTitle) {\n\t\tif (cell.classList.contains('selected')) {\n\t\t\tcell.classList.remove('selected');\n\t\t\tselectedPodcasts = selectedPodcasts.filter(title => title !== podcastTitle);\n\t\t} else if (selectedPodcasts.length < 6) {\n\t\t\tcell.classList.add('selected');\n\t\t\tselectedPodcasts.push(podcastTitle);\n\t\t}\n\t\tsaveSelectedPodcasts();\n\t\tcheckSelections();\n\t}\n\n\t\/\/ Function to save selected podcasts as embedded data\n\tfunction saveSelectedPodcasts() {\n\t\tfor (let i = 0; i < selectedPodcasts.length; i++) {\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('choice-' + (i + 1), selectedPodcasts[i]);\n\t\t}\n\t\t\/\/ If less than 6 podcasts are selected, clear the remaining embedded data\n\t\tfor (let i = selectedPodcasts.length; i < 6; i++) {\n\t\t\tQualtrics.SurveyEngine.setEmbeddedData('choice-' + (i + 1), '');\n\t\t}\n\t}\n\n    \/\/ Function to check whether 6 podcasts are selected\n    function checkSelections() {\n        let nextButton = document.querySelector('#NextButton');\n        if (selectedPodcasts.length === 6) {\n            nextButton.style.pointerEvents = 'auto';\n            nextButton.style.opacity = 1;\n        } else {\n            nextButton.style.pointerEvents = 'none';\n            nextButton.style.opacity = 0.5;\n        }\n    }\n\n    \/\/ Initially disable the NextButton\n    checkSelections();\n\n    \/\/ Create table cells for podcasts\n    for (var i = 0; i < rows; i++) {\n        var row = document.createElement('tr');\n        for (var j = 0; j < cols; j++) {\n            var index = i * cols + j;\n            var cell = document.createElement('td');\n\n            if (index < podcasts.length) {\n                var podcast = podcasts[index];\n                var img = document.createElement('img');\n                img.src = podcast.link;\n                img.style.width = '100px';\n                img.style.height = '150px';\n                img.style.margin = '0 auto';\n\n                cell.innerHTML = img.outerHTML + \"<br>\" +\n                                 \"<b>Hosted by <\/b><br>\" +\n                                 \"<u>\" + podcast.host + \"<\/u>\" + \" (\" + podcast.bio + \")\" + \"<br>\" +\n                                 \"<u>Start Year:<\/u> \" + podcast.start + \"<br>\" +\n                                 \"<u>Episodes:<\/u> \" + podcast.count + \"<br>\" +\n                                 \"<u>Avg. Duration:<\/u> \" + podcast.duration + \"\";\n\n                cell.addEventListener('click', function(podcastTitle) {\n                    return function() {\n                        handlePodcastClick(this, podcastTitle);\n                    };\n                }(podcast.title));\n            }\n\n            cell.style.border = '1px solid black';\n            cell.style.padding = '10px';\n            cell.style.cursor = 'pointer';\n            cell.style.textAlign = 'center';\n            cell.style.verticalAlign = 'top';\n            row.appendChild(cell);\n        }\n        table.appendChild(row);\n    }\n\n    \/\/ Add styles for the selected class\n    var style = document.createElement('style');\n    style.innerHTML = `\n        td.selected {\n            background-color: #ffdd99;\n        }\n    `;\n    document.head.appendChild(style);\n\n    \/\/ Add the table to the survey question\n    this.getQuestionContainer().appendChild(table);\n});\n\nQualtrics.SurveyEngine.addOnReady(function() {\n    \/\/ Logic to run when t