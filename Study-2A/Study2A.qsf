f (author && previousChoices.includes(author.name)) {\n                cell.classList.add('previous-choice');\n                cell.style.backgroundColor = 'gray';\n            }\n\t\t\tif (index < authors.length) {\n\t\t\t\tvar author = authors[index];\n\n\t\t\t\t\/\/ Create img element and add the link\n\t\t\t\tvar img = document.createElement('img');\n\t\t\t\timg.src = author.photoUrl;\n\t\t\t\timg.style.width = '150px';\n\t\t\t\timg.style.height = '150px';\n\t\t\t\timg.style.margin = '0 auto'; \/\/ To center the image\n\n\t\t\t\t\/\/ Add img and other details to cell\n\t\t\t\tcell.innerHTML = img.outerHTML + \"<br>\" + \n\t\t\t\t\t\"<b>\" + author.name + \"<\/b><br>\" + \n\t\t\t\t\t\"<u>Birth Year:<\/u> \" + author.birthYear + \"<br>\" + \n\t\t\t\t\t\"<u>Best-Known Work:<\/u> \" + author.bestKnownWork + \"<br>\" +\n\t\t\t\t\t\"<u>Authored:<\/u> \" + author.authoredWorks;\n\n\t\t\t\t\/\/ Add click event to select the author from the table\n\t\t\t\tcell.addEventListener('click', function(authorName) {\n\t\t\t\t\treturn function() {\n\t\t\t\t\t\thandleAuthorClick(this, authorName);\n\t\t\t\t\t};\n\t\t\t\t}(author.name)); \/\/ Pass the current author name into the event function\n\t\t\t}\n\n\t\t\t\/\/ Style cell\n\t\t\tcell.style.border = '1px solid black';\n\t\t\tcell.style.padding = '10px';\n\t\t\tcell.style.cursor = 'pointer';\n\t\t\tcell.style.textAlign = 'center';     \/\/ Center align content horizontally\n\t\t\tcell.style.verticalAlign = 'top';    \/\/ Top align content vertically\n\t\t\trow.appendChild(cell);\n\t\t}\n\t\ttable.appendChild(row);\n\t}\n\t\n\t\/\/ Check if author is in the previousChoices\n\tif (previousChoices.includes(author.name)) {\n\t\tcell.classList.add('previous-choice');\n\t\tcell.style.border = '1px solid black';\n        cell.style.padding = '10px';\n        cell.style.cursor = 'pointer';\n        cell.style.textAlign = 'center';     \/\/ Center align content horizontally\n        cell.style.verticalAlign = 'top';    \/\/ Top align content vertically\n\t}\n\n\t\n\t\n\n\/\/ Additional styles for the previous-choice class\nvar style = document.createElement('style');\nstyle.innerHTML = `\n    td.previous-choice {\n        background-color: #CCCCCC; \/\/ Gray color for already selected choices\n        cursor: not-allowed;\n    }\n    td.selected {\n        background-color: #ffdd99; \/\/ Light green for the newly selected choice\n    }\n`;\n\n\tdocument.head.appendChild(style);\n\t    \/\/ Add the table to the survey question\n    this.getQuestionContainer().appendChild(table);\n});\n\nQualtrics.SurveyEngine.addOnReady(function()\n{\n\t\/*Place your JavaScript here to run when the page is fully displayed*\/\n\n});\n\nQualtrics.SurveyEngine.addOnUnload(function()\n{\n\t\/*Place your JavaScript here to run when the page is unloaded*\/\n\n});","DataVisibility":{"Private":false,"Hidden":false}}},{"SurveyID":"SV_e4eMITeZ8WisKSa","Element":"SQ","PrimaryAttribute":"QID4","SecondaryAttribute":"You\u2019ve picked the first six authors for your reading list, but we actually have room for seven. O.","TertiaryAttribute":null,"Payload":{"QuestionText":"<b>You\u2019ve picked the first six authors for your reading list, but we actually have room for seven. On the next page we will ask you to make your final selection.<\/b><br>","DefaultChoices":false,"DataExportTag":"Q4","QuestionID":"QID4","QuestionType":"DB","Selector":"TB","Configuration":{"QuestionDescriptionOption":"UseText"},"QuestionDescription":"You\u2019ve picked the first six authors for your reading list, but we actually have room for seven. O...","ChoiceOrder":[],"Validation":{"Settings":{"Type":"None"}},"GradingData":[],"Language":[],"NextChoiceId":4,"NextAnswerId":1,"DataVisibility":{"Private":false,"Hidden":false}}}]}