tinuous print 50+ years (unchanged)\n  classic_50plus: [\n    \"Jane Austen\",\"Charles Dickens\",\"Herman Melville\",\"Nathaniel Hawthorne\",\n    \"Jack London\",\"J.D. Salinger\",\"F. Scott Fitzgerald\",\"Ernest Hemingway\",\n    \"John Steinbeck\",\"George Orwell\",\"J.R.R. Tolkien\",\"Lucy Maud\",\n    \"Ray Bradbury\",\"Kurt Vonnegut\",\"Gabriel Garcia Marquez\",\n    \"Michael Crichton\",\"Sylvia Plath\"\n  ]\n};\n\n\/*******************************************\n * Card content config & author metadata\n *******************************************\/\nvar NOTABLE_LABEL = \"Most notable work\";\n\n\/\/ Use the exact titles + copy counts you provided.\n\/\/ startYear is the publication year for that specific work.\n\/\/ copiesSold accepts \"K\" and \"M\" (e.g., \"657K\", \"1.5M\") and is used verbatim on cards.\nvar AUTHOR_META = {\n  \"Zadie Smith\":            { title:\"White Teeth\",                    startYear:2000, copiesSold:\"6M\",   genres:[\"Literary Fiction\"],                                           booksCount:6,  poemsCount:null, essaysCount:1 },\n  \"Charles Dickens\":        { title:\"A Tale of Two Cities\",           startYear:1859, copiesSold:\"200M\", genres:[\"Realism\"],                                                    booksCount:15, poemsCount:1,    essaysCount:3 },\n  \"Ernest Hemingway\":       { title:\"The Old Man and the Sea\",        startYear:1952, copiesSold:\"13M\",  genres:[\"Literary Fiction\"],                                          booksCount:7,  poemsCount:1,    essaysCount:2 },\n  \"Gabriel Garcia Marquez\": { title:\"One Hundred Years of Solitude\",  startYear:1967, copiesSold:\"50M\",  genres:[\"Magical Realism\",\"Literary Fiction\"],                        booksCount:6,  poemsCount:null, essaysCount:1 },\n  \"George Orwell\":          { title:\"1984\",                           startYear:1949, copiesSold:\"30M\",  genres:[\"Dystopia\",\"Satire\"],                                         booksCount:6,  poemsCount:1,    essaysCount:6 },\n  \"Herman Melville\":        { title:\"Moby-Dick\",                      startYear:1851, copiesSold:\"8M\",   genres:[\"Adventure\",\"Gothic\"],                                        booksCount:11, poemsCount:1,    essaysCount:1 },\n  \"Isabel Allende\":         { title:\"The House of the Spirits\",       startYear:1982, copiesSold:\"75M\",  genres:[\"Magical Realism\",\"Literary Fiction\"],                       booksCount:25, poemsCount:null, essaysCount:1 },\n  \"Jack London\":            { title:\"The Call of the Wild\",           startYear:1903, copiesSold:\"7M\",   genres:[\"Adventure\"],                                                  booksCount:23, poemsCount:1,    essaysCount:1 },\n  \"Jane Austen\":            { title:\"Pride and Prejudice\",            startYear:1813, copiesSold:\"20M\",  genres:[\"Romance\"],                                                   booksCount:6,  poemsCount:null, essaysCount:null },\n  \"J.D. Salinger\":          { title:\"The Catcher in the Rye\",         startYear:1951, copiesSold:\"65M\",  genres:[\"Literary Fiction\"],                                          booksCount:4,  poemsCount:1,    essaysCount:null },\n  \"John Steinbeck\":         { title:\"The Grapes of Wrath\",            startYear:1939, copiesSold:\"14M\",  genres:[\"Realism\"],                                                    booksCount:33, poemsCount:null, essaysCount:1 },\n  \"Joyce Carol Oates\":      { title:\"We Were the Mulvaneys\",          startYear:1996, copiesSold:\"75K\",  genres:[\"Gothic\",\"Crime Fiction\"],                                    booksCount:58, poemsCount:1,    essaysCount:5 },\n  \"Jorge Luis Borges\":      { title:\"Ficciones\",                      startYear:1944, copiesSold:\"2M\",   genres:[\"Literary Fiction\"],                                          booksCount:74, poemsCount:3,    essaysCount:5 },\n  \"J.R.R. Tolkien\":         { title:\"The Lord of the Rings\",          startYear:1954, copiesSold:\"150M\", genres:[\"Fantasy\",\"Adventure\"],                                       booksCount:28, poemsCount:1,    essaysCount:1 },\n  \"Lucy Maud\":              { title:\"Anne of Green Gables\",           startYear:1908, copiesSold:\"50M\",  genres:[\"Children's Literature\",\"Coming-of-Age\"],                    booksCount:20, poemsCount:1,    essaysCount:1 },\n  \"Michael Crichton\":       { title:\"Jurassic Park\",                  startYear:1990, copiesSold:\"9M\",   genres:[\"Techno-thriller\"],                                           booksCount:32, poemsCount:null, essaysCount:1 },\n  \"Nathaniel Hawthorne\":    { title:\"The Scarlet Letter\",             startYear:1850, copiesSold:\"10M\",  genres:[\"Gothic\"],                                                     booksCount:8,  poemsCount:1,    essaysCount:1 },\n  \"Sylvia Plath\":           { title:\"The Bell Jar\",                   startYear:1971, copiesSold:\"3M\",   genres:[\"Literary Fiction\"],                                          booksCount:4,  poemsCount:2,    essaysCount:1 },\n  \"Kurt Vonnegut\":          { title:\"Slaughterhouse-Five\",            startYear:1969, copiesSold:\"800K\", genres:[\"Science Fiction\",\"Satire\"],                                  booksCount:14, poemsCount:null, essaysCount:3 },\n  \"Ray Bradbury\":           { title:\"Fahrenheit 451\",                 startYear:1953, copiesSold:\"10M\",  genres:[\"Science Fiction\",\"Fantasy\"],                                 booksCount:27, poemsCount:3,    essaysCount:1 },\n  \"Neil Gaiman\":            { title:\"American Gods\",                  startYear:2001, copiesSold:\"1M\",   genres:[\"Fantasy\",\"Horror\"],                                          booksCount:5,  poemsCount:1,    essaysCount:1 },\n  \"Cormac McCarthy\":       { title:\"The Road\",                        startYear:2006, copiesSold:\"657K\", genres:[\"Western\",\"Southern Gothic\"],                                 booksCount:10, poemsCount:null, essaysCount:1 },\n  \"George Saunders\":        { title:\"Lincoln in the Bardo\",           startYear:2017, copiesSold:\"1M\",   genres:[\"Literary Fiction\"],                                          booksCount:1,  poemsCount:null, essaysCount:1 },\n  \"F. Scott Fitzgerald\":    { title:\"The Great Gatsby\",               startYear:1925, copiesSold:\"30M\",  genres:[\"Literary Fiction\"],                                          booksCount:4,  poemsCount:1,    essaysCount:1 },\n  \"Ian McEwan\":             { title:\"Atonement\",                      startYear:2001, copiesSold:\"1.5M\", genres:[\"Literary Fiction\"],                                          booksCount:17, poemsCount:null, essaysCount:1 }\n};\n\n\/\/ Parse \"K\/M\" copies like \"657K\", \"1.5M\" \u2192 number\nfunction parseCopiesStr(s){\n  if (!s) return 0;\n  var x = String(s).trim().toUpperCase();\n  var mult = 1;\n  if (x.endsWith(\"M\")) { mult = 1e6; x = x.slice(0, -1); }\n  else if (x.endsWith(\"K\")) { mult = 1e3; x = x.slice(0, -1); }\n  var num = parseFloat(x);\n  return isNaN(num) ? 0 : num * mult;\n}\n\n\/\/ Build sold_30m from AUTHOR_META so thumbnails and feedback match\n(function buildSold30m(){\n  var s = [];\n  for (var name in AUTHOR_META){\n    if (Object.prototype.hasOwnProperty.call(AUTHOR_META, name)){\n      var n = parseCopiesStr(AUTHOR_META[name].copiesSold);\n      if (n >= 30e6) s.push(name);\n    }\n  }\n  categories.sold_30m = s;\n})();\n\n\/***********************\n * Qualtrics question JS\n ***********************\/\nQualtrics.SurveyEngine.addOnReady(function () {\n  var seed = Math.floor(Math.random() * 100000);\n  Qualtrics.SurveyEngine.setEmbeddedData('randomSeed', seed);\n\n  function shuffleArray(a, s) {\n    var m = a.length, t, i;\n    function rnd(){ var x = Math.sin(s++) * 10000; return x - Math.floor(x); }\n    while (m) { i = Math.floor(rnd() * m--); t = a[m]; a[m] = a[i]; a[i] = t; }\n  }\n\n  function Author(photoUrl, name, birthYear, bestKnownWork, authoredWorks) {\n