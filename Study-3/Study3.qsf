or (var j = 0; j < cols; j++) {\n\t\t\tvar index = i * cols + j;\n\n\t\t\t\/\/ Check if the index is within the range of names and it is less than 26\n\t\t\tif (index < names.length && index < 26) {\n\t\t\t\tvar splitName = names[index].split(' (');\n\t\t\t\tvar name = '<b>' + splitName[0] + '<\/b>';\n\t\t\t\tvar title = splitName[1].slice(0, -1);\n\n\t\t\t\t\/\/ Check if person is a technologist, if yes append superscript\n\t\t\t\tif (technologists.includes(splitName[0])) {\n\t\t\t\t\ttitle += \"<sup>\u2020<\/sup>\";\n\t\t\t\t}\n\n\t\t\t\tvar cell = document.createElement('td');\n\t\t\t\tcell.innerHTML = name + \"<br>\" + title;\n\n\t\t\t\t\/\/ Style cell\n\t\t\t\tcell.style.border = '1px solid black';\n\t\t\t\tcell.style.padding = '10px';\n\t\t\t\trow.appendChild(cell);\n\t\t\t} else {\n\t\t\t\tvar cell = document.createElement('td');\n\t\t\t\tcell.innerHTML = \"\";\n\t\t\t\tcell.style.border = '1px solid black';\n\t\t\t\trow.appendChild(cell);\n\t\t\t}\n\t\t}\n\t\ttable.appendChild(row);\n\t}\n\n    \/\/ Add the table to the survey question\n    this.getQuestionContainer().appendChild(table);\n\t\n    \/\/ At the end of your code, add a paragraph as a footnote\n    var footnote = document.createElement('p');\n    footnote.style.fontSize = '0.8em'; \/\/ Makes the font size 80% of the normal font size\n    footnote.style.textAlign = 'right'; \/\/ Right-aligns the text\n    footnote.innerHTML = \"<br><sup>\u2020<\/sup> denotes a technology company.<br>\";\n    this.getQuestionContainer().appendChild(footnote);\n\n    \/\/ Instruction paragraph\n    var instructionPara = document.createElement('p');\n    instructionPara.innerHTML = \"<br>Please select the <b>6 people<\/b> you would like to include on your expert panel.<br><br>\";\n    this.getQuestionContainer().appendChild(instructionPara);\n\n    \/\/ Create dropdown questions\n    var dropdowns = [];\n    for (let i = 1; i <= 6; i++) {\n        var label = document.createElement('label');\n        label.innerHTML = 'Selection ' + i + ': ';\n        var dropdown = document.createElement('select');\n\t\tdropdown.style.height = '35px'; \/\/ CSS to modify dropdown height\n\t\tdropdown.style.width = '450px'; \/\/ CSS to modify dropdown width\n\t\tdropdown.style.fontSize = '16px'; \/\/ CSS to modify dropdown font size\n\t\tdropdown.style.padding = '3px'; \/\/ CSS to modify dropdown padding\n\t\tdropdown.style.border = '1px solid #000'; \/\/ CSS to modify dropdown border\n\n        \/\/ Add empty option to the dropdown\n        var option = document.createElement('option');\n        option.value = '';\n        option.textContent = '';\n        dropdown.appendChild(option);\n\n        \/\/ Add names to dropdown\n        for (let name of names) {\n            var option = document.createElement('option');\n            option.value = name;\n            option.textContent = name;\n            dropdown.appendChild(option);\n        }\n\n        \/\/ Add dropdown to array for reference\n        dropdowns.push(dropdown);\n\n        \/\/ Add dropdown to question container\n        this.getQuestionContainer().appendChild(label);\n        this.getQuestionContainer().appendChild(dropdown);\n        this.getQuestionContainer().appendChild(document.createElement('br'));\n    }\n\n    \/\/ Create warning message\n    var warningMsg = document.createElement('p');\n    warningMsg.id = 'warning';\n    warningMsg.style.color = 'red';\n    warningMsg.innerHTML = \"\";\n    warningMsg.hidden = true; \/\/ Hide the warning message by default\n    this.getQuestionContainer().appendChild(warningMsg);\n\n    \/\/ Disable duplicate selections across dropdowns\n    for (let i = 0; i < dropdowns.length; i++) {\n        dropdowns[i].addEventListener('change', function() {\n            \/\/ Check for duplicates\n            let selectedValue = this.value;\n            for (let j = 0; j < dropdowns.length; j++) {\n                if (i === j) continue; \/\/ Skip checking the same dropdown\n\n                if (dropdowns[j].value === selectedValue) {\n                    \/\/ Duplicates found, show warning message\n                    warningMsg.innerHTML = \"Please ensure selections are unique.\";\n                    warningMsg.hidden = false;\n\n                    \/\/ Reset the current dropdown\n                    this.value = '';\n                    return; \/\/ Exit the loop as soon as a duplicate is found\n                }\n            }\n            \/\/ No duplicates found, hide the warning message\n            warningMsg.hidden = true;\n        });\n    }\n\n    \/\/ Initial check and set of NextButton status\n    checkDropdowns();\n\n    \/\/ Event listener to check dropdowns whenever any dropdown changes\n    dropdowns.forEach(function(dropdown) {\n        dropdown.addEventListener('change', checkDropdowns);\n    });\n\n    \/\/ Function to check whether all dropdowns have a selection\n    function checkDropdowns() {\n        let allFilled = dropdowns.every(function(dropdown) {\n            return dropdown.value !== '';\n        });\n\n        \/\/ Get the NextButton and modify its properties\n        let nextButton = document.querySelector('#NextButton');\n        if (allFilled) {\n            nextButton.style.pointerEvents = 'auto';\n            nextButton.style.opacity = '1';\n        } else {\n            nextButton.style.pointerEvents = 'none';\n            nextButton.style.opacity = '0.5';\n        }\n    }\n\t\n\t\n\t\/\/ assuming dropdowns is an array of your dropdown elements\ndropdowns.forEach(function(dropdown, index) {\n    \/\/ +1 because the index starts from 0 and your embedded variables start from 1\n    var embeddedVariableName = 'choice-' + (index + 1); \n\n    \/\/ add an event listener to the change event\n    dropdown.addEventListener('change', function() {\n        \/\/ this.value is the selected value of the dropdown\n        Qualtrics.SurveyEngine.setEmbeddedData(embeddedVariableName, this.value);\n    });\n\tQualtrics.SurveyEngine.setEmbeddedData('sample', names.join('||'));\n\n});\n\n});\nQualtrics.SurveyEngine.addOnReady(function()\n{\n\t\/*Place your JavaScript here to run when the page is fully displayed*\/\n\n});\n\nQualtrics.SurveyEngine.addOnUnload(function()\n{\n\t\/*Place your JavaScript here to run when the page is unloaded*\/\n\n});","DefaultChoices":false,"DataExportTag":"pool","QuestionType":"DB","Selector":"TB","DataVisibility":{"Private":false,"Hidden":false},"Configuration":{"QuestionDescriptionOption":"UseText"},"QuestionDescription":"Imagine you\u2019re planning a major international business event. You are working to convene a panel...","ChoiceOrder":[],"Validation":{"Settings":{"Type":"None"}},"GradingData":[],"Language":[],"NextChoiceId":4,"NextAnswerId":1,"QuestionID":"QID5"}},{"SurveyID":"SV_8uMcrtMJwiIR6EC","Element":"SQ","PrimaryAttribute":"QID9","SecondaryAttribute":"Informed Consent You are being asked to complete a survey as part of a research study about human...","TertiaryAttribute":null,"Payload":{"ChoiceOrder":[],"Configuration":{"QuestionDescriptionOption":"UseText"},"DataExportTag":"consent","DataVisibility":{"Hidden":false,"Private":false},"DefaultChoices":false,"GradingData":[],"Language":[],"NextAnswerId":1,"NextChoiceId":4,"QuestionDescription":"Informed Consent You are being asked to complete a survey as part of a research study about human...","QuestionID":"QID9","QuestionText":"<div style=\"text-align: center;\"><b>Informed Consent<\/b><\/div>\n<br>\nYou are being asked to complete a survey as part of a research study about human behavior and decision-making. Your participation in this study is entirely voluntary. If you elect to participate, you will be asked to select an advisory panel based on a pool of options. You will then be asked to answer a series of survey questions. Additional information will be provided to you during your study participation.<br>\n<br>\nThis survey should take about 3 minutes. You can withdraw from the study at any time. If you decide to withdraw from the survey, your data will be destroyed. We will not share or store any data beyond the use of this research project.<br>\n<br>\nWe will make every effort to keep all the information you tell us during the study strictly confidential, except as required by law. Worker IDs will be collected strictly on the basis of compensation. Your name and other identifying information will never be connected with the responses you provide so no one will ever be able to identify you in any publications that will result from this research. If you have questions, concerns, or complaints regarding your confidentiality or if you have any questions about your rights as a research participant, you should e-mail the researcher at josecerv@wharton.upenn.edu. If a member of the research team cannot be reached or you want to talk to someone other than those working on the study, you may contact the Office of Regulatory Affairs with any questions, concerns, or complaints at the University of Pennsylvania by calling (215) 898-2614.<br>\n<br>\nIf you agree to participate, please click the arrow to continue. Otherwise, you may close the browser or navigate away from the page.","QuestionText_Unsafe":"<div style=\"text-align: center;\"><b>Informed Consent<\/b><\/div>\n<br>\nYou are being asked to complete a survey as part of a research study about human behavior and decision-making. Your participation in this study is entirely voluntary. If you elect to participate, you will be asked to select an advisory panel based on a pool of options. You will then be asked to answer a series of survey questions. Additional information will be provided to you during your study participation.<br>\n<br>\nThis survey should take about 5 minutes. You can withdraw from the study at any time. If you decide to withdraw from the survey, your data will be destroyed. We will not share or store any data beyond the use of this research project.<br>\n<br>\nWe will make every effort to keep all the information you tell us during the study strictly confidential, except as required by law. Worker IDs will be collected strictly on the basis of compensation. Your name and other identifying information will never be connected with the responses you provide so no one will ever be able to identify you in any publications that will result from this research. If you have questions, concerns, or complaints regarding your confidentiality or if you have any questions about your rights as a research participant, you should e-mail the researcher at josecerv@wharton.upenn.edu. If a member of the research team cannot be reached or you want to talk to someone other than those working on the study, you may contact the Office of Regulatory Affairs with any questions, concerns, or complaints at the University of Pennsylvania by calling (215) 898-2614.<br>\n<br>\nIf you agree to participate, please click the arrow to continue. Otherwise, you may close the browser or navigate away from the page.","QuestionType":"DB","Selector":"TB","Validation":{"Settings":{"Type":"None"}}}},{"SurveyID":"SV_8uMcrtMJwiIR6EC","Element":"SQ","PrimaryAttribute":"QID6","SecondaryAttribute":"Please check the box below to proceed.","TertiaryAttribute":null,"Payload":{"Configuration":{"Align":"left","QuestionDescriptionOption":"UseText","Theme":"light"},"DataExportTag":"captcha","DefaultChoices":false,"GradingData":[],"Language":[],"NextAnswerId":1,"NextChoiceId":4,"QuestionDescription":"Please check the box below to proceed.","QuestionID":"QID6","QuestionText":"Please check the box below to proceed.","QuestionText_Unsafe":"Please check the box below to proceed.","QuestionType":"Captcha","Selector":"V2","QuestionJS":false,"DataVisibility":{"Private":false,"Hidden":false}}},{"SurveyID":"SV_8uMcrtMJwiIR6EC","Element":"SQ","PrimaryAttribute":"QID8","SecondaryAttribute":"Please ignore the question below and instead type in \"one one\" (exactly as shown) as your respons...","TertiaryAttribute":null,"Payloa